{
  "openrouter": {
    "api_base_url": "https://openrouter.ai/api/v1/chat/completions",
    "api_key_var": "OPENROUTER_API_KEY",
    "transformer": {
      "use": ["openrouter"]
    },
    "description": "Access 200+ models through OpenRouter's unified API",
    "website": "https://openrouter.ai"
  },
  "deepseek": {
    "api_base_url": "https://api.deepseek.com/chat/completions",
    "api_key_var": "DEEPSEEK_API_KEY",
    "transformer": {
      "use": ["deepseek"]
    },
    "description": "DeepSeek AI models (chat and reasoning)",
    "website": "https://www.deepseek.com"
  },
  "gemini": {
    "api_base_url": "https://generativelanguage.googleapis.com/v1beta/models/",
    "api_key_var": "GOOGLE_API_KEY",
    "transformer": {
      "use": ["gemini"]
    },
    "description": "Google Gemini models (Flash and Pro)",
    "website": "https://ai.google.dev"
  },
  "openai": {
    "api_base_url": "https://api.openai.com/v1/chat/completions",
    "api_key_var": "OPENAI_API_KEY",
    "transformer": {
      "use": []
    },
    "description": "OpenAI GPT models",
    "website": "https://openai.com"
  },
  "anthropic": {
    "api_base_url": "https://api.anthropic.com/v1/messages",
    "api_key_var": "ANTHROPIC_API_KEY",
    "transformer": {
      "use": ["Anthropic"]
    },
    "description": "Anthropic Claude models (requires API key, not login)",
    "website": "https://anthropic.com"
  },
  "perplexity": {
    "api_base_url": "https://api.perplexity.ai/chat/completions",
    "api_key_var": "PERPLEXITY_API_KEY",
    "transformer": {
      "use": []
    },
    "description": "Perplexity AI models (Sonar with online search)",
    "website": "https://www.perplexity.ai"
  },
  "minimax": {
    "api_base_url": "https://api.minimax.chat/v1/text/chatcompletion_v2",
    "api_key_var": "MINIMAX_API_KEY",
    "transformer": {
      "use": []
    },
    "description": "MiniMax AI models including MiniMax-M2",
    "website": "https://www.minimaxi.com"
  },
  "kimi": {
    "api_base_url": "https://api.moonshot.cn/v1/chat/completions",
    "api_key_var": "KIMI_API_KEY",
    "transformer": {
      "use": []
    },
    "description": "Kimi (Moonshot AI) models including Kimi K2",
    "website": "https://platform.moonshot.cn"
  },
  "volcengine": {
    "api_base_url": "https://ark.cn-beijing.volces.com/api/v3/chat/completions",
    "api_key_var": "VOLCENGINE_API_KEY",
    "transformer": {
      "use": ["deepseek"]
    },
    "description": "Volcengine (ByteDance) AI models",
    "website": "https://www.volcengine.com"
  },
  "siliconflow": {
    "api_base_url": "https://api.siliconflow.cn/v1/chat/completions",
    "api_key_var": "SILICONFLOW_API_KEY",
    "transformer": {
      "use": [
        ["maxtoken", {"max_tokens": 16384}]
      ]
    },
    "description": "SiliconFlow AI cloud platform",
    "website": "https://siliconflow.cn"
  },
  "modelscope": {
    "api_base_url": "https://api-inference.modelscope.cn/v1/chat/completions",
    "api_key_var": "MODELSCOPE_API_KEY",
    "transformer": {
      "use": [
        ["maxtoken", {"max_tokens": 65536}],
        "enhancetool"
      ]
    },
    "description": "ModelScope (Alibaba) AI platform",
    "website": "https://www.modelscope.cn"
  },
  "dashscope": {
    "api_base_url": "https://dashscope-intl.aliyuncs.com/compatible-mode/v1/chat/completions",
    "api_key_var": "DASHSCOPE_API_KEY",
    "transformer": {
      "use": [
        ["maxtoken", {"max_tokens": 65536}],
        "enhancetool"
      ]
    },
    "description": "DashScope (Alibaba Cloud) Qwen models - International",
    "website": "https://dashscope.aliyun.com"
  },
  "llamacpp-local": {
    "api_base_url": "$LLM_INFERENCE_LLAMACPP_URL/chat/completions",
    "api_key": "not-needed",
    "transformer": {
      "use": []
    },
    "description": "Local llama.cpp server (Container 100) - GGUF models",
    "website": "local"
  },
  "ollama-local": {
    "api_base_url": "$LLM_INFERENCE_OLLAMA_URL/chat/completions",
    "api_key": "ollama",
    "transformer": {
      "use": []
    },
    "description": "Local Ollama server (Container 100) - cloud models",
    "website": "local"
  }
}
